{
    "extends": "tslint-config-airbnb",
    "rules": {
        "import-name": false,
        "max-line-length": false,
        "space-in-parens": false,
        "ter-indent": 4,
        "object-curly-spacing": false,
        "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
        "prefer-template": false,
        "object-literal-shorthand": [true, "never"]
    }
}
